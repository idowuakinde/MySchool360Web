		<add name="iTecon_School" connectionString="Database=SmartSchoolChildville;Server=.;Integrated Security=SSPI;Connection Timeout=300;" providerName="System.Data.SqlClient"/>
	</connectionStrings>
	<appSettings>
		<add key="Diagnostics_IP_Local" value="127.0.0.1"/>
		<add key="Diagnostics_IP_Internet" value="72.14.207.99"/>
		<add key="Application_Title" value="MySchool360"/>
